		"custom": {
			"border-radius": {
				"0": "0",
				"025": "4px",
				"05": "8px",
				"1": "16px",
				"1-5": "24px",
				"2": "32px",
				"rounded": "999px"
			},
			"box-shadow": {
				"1": "0px 2px 8px rgba(33, 33, 33, 0.12)",
				"2": "3px 3px 10px 10px rgba(0, 0, 0, 0.10)"
			},
			"layout": {
				"block-gap": "2rem",
				"block-gap-large": "40px",
